<?xml version="1.0" encoding="UTF-8"?>
<glassfish-ejb-jar xmlns="https://jakarta.ee/xml/ns/jakartaee"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/glassfish-ejb-jar_7_0.xsd"
                   version="7.0">
    <enterprise-beans>
        <ejb>
            <ejb-name>PersonaServiceImpl</ejb-name>
            <webservice-endpoint>
                <port-component-name>PersonaServiceImpl</port-component-name>
                <login-config>
                    <auth-method>BASIC</auth-method>
                </login-config>
            </webservice-endpoint>
        </ejb>
    </enterprise-beans>
</glassfish-ejb-jar>
